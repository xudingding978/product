function lineheight_px(obj){$(obj).append("<div id='temp_ellipsis_div' style='position:absolute; visibility:hidden'>H</div>");var temp_height=$('#temp_ellipsis_div').height();$('#temp_ellipsis_div').remove();return temp_height;}
