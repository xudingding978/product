
 
<div class="profilegb width-100 height-auto" >
    <div class="landing-page"> 
    <img src="../../../images/landingpagebg.jpg">
</div>

    <div class="profilenew-title-container">

        <img src="http://s3.hubsrv.com/trendsideas.com/profiles/trends-media-sales-marketing-services/profile_picture/Screen%20Shot%202013-09-05%20at%209_286x284.51" style="width: 150px; height:auto;border: 3px solid white;box-shadow: 0px 0px 15px #555; float: left;">

        <div class="profilenew-title-txt">
            <p><b>SIGN UP NOW</b></p> 
            <p><b>TO myTrends</b></p>        
        </div>
    </div>

    <div class="profilewrapper profilenew-content-container" >

        <div class="profilecorpbanner profilenewbanner" >       
            <img src="https://s3-ap-southeast-2.amazonaws.com/develop.devbox/profile_cover/default/defaultcover4.jpg" class="width-100"/>
            <div class="corpbanner_mask profilenew-bannermark" ></div>
        </div>

        <div class="profilenew-steptitle"> Step 1. Package</div>
        <div class="contactme-table easing profilenew-eachblock profilenew-block1" >
            
            <div id="gold" class="profilenew-package hover-opacity easing cursor-pointer" {{action packageSelection "gold"}}>GOLD</div>
            <div  id="silver" class="profilenew-package hover-opacity easing cursor-pointer" {{action packageSelection "silver"}}>SILVER</div>
            <div  id="bronze" class="profilenew-package hover-opacity easing cursor-pointer" {{action packageSelection "bronze"}}>BRONZE</div>

        </div>
        <div class="profilenew-steptitle">Step 2. Client Detail</div>

        <div class="mustfull display-none" id="errorMessage2" >please fill in Client Email. </div>
        <div class="mustfull display-none" id="errorMessage1" >please fill in Client Name. </div>
    
        <div class="contactme-table easing profilenew-eachblock profilenew-block2">

            <div class="profilenew-step-container">
                              
                <div class="table-row"></div>

                <div class="table-row">
                    <div class="table-cell align-right" >Client Name*:</div>
                    <div class="table-cell"> 
                        <div class="display-block">
                            <div  id="clientNameField" class="profilenew-inputarea" >
                                {{view Ember.TextField valueBinding="client_name" class="clientName"  placeholder="Your Name" }}
                            </div>
                        </div>

                    </div>
                </div>

                <div class="table-row">
                    <div class="table-cell align-right" >Client Email*:</div>

                    <div class="table-cell"> 
                        <div class="display-block">
                            <div   id="clientEmailField" class="profilenew-inputarea">
                                {{view Ember.TextField valueBinding="owner" class="clientEmail"  placeholder="emailaddress@yourdomain.com"}}
                            </div>
                        </div>   
                        <div class="mustfull display-none" id="clientEmailFormat" >not correct email format.....</div>

                    </div>
                </div>
                
            </div>
        </div>
        <div class="profilenew-steptitle">Step 3. Business Profile</div>

        <div class="mustfull display-none" id="errorMessage3" >please fill in Profile Name. </div>
        <div class="mustfull display-none" id="errorMessage5" >please fill in Contact Email. </div>
        <div class="mustfull display-none" id="errorMessage6" >please fill in Admins Emails. </div>
        <div class="mustfull display-none" id="numberFormat" >please fill in info as number </div>
        <div class="mustfull display-none" id="errorMessage4" >please select one from the dropdown list </div>
        <div class="mustfull display-none" id="errorMessage7" >please select one classfication </div>
        <div class="contactme-table easing profilenew-eachblock profilenew-block3" >

            <div id="step3" class="profilenew-step-container">
           

                    <div class="table-row">  
                    </div>
                
                 <div class="table-row">
                    <div class="table-cell align-right" >Profile URL*:</div>
                    <div class="table-cell" > 
                        <div class="profilenew-inputarea align-center" >
                            {{form controller.profile_name}}-{{form controller.countrySelection}}
                        </div>
                  
                    </div> 
                </div>

                    <div class="table-row">
                        <div class="table-cell align-right" >Profile Name*: </div>                     
                        <div class="table-cell"> 
                            <div  id="profileNameField" class="profilenew-inputarea">
                                {{view Ember.TextField valueBinding="profile_name" class="profileName" placeholder="Your Business Name"}} 
                            </div>
                        </div>
                    </div>
                
                 <div class="table-row">
                        <div class="table-cell align-right" >Classification*:</div> 
                        <div  class="table-cell">
                     
                                <div id="residential" class="classfication profilenew-input-halfarea display-inline" {{action classSelection "Residential"}}>
                                     <span id="">Residential</span>
                                    
                                </div>
                                <div id="commercial" class="classfication profilenew-input-halfarea2 display-inline" {{action classSelection "Commercial"}}>
                                     <span id="">Commercial</span>
                                   
                                   
                                </div>                              
                         
                        </div>
                    </div>


                    <div class="table-row">
                        <div class="table-cell align-right" >Profile Category*:</div> 
                        <div class="table-cell">
                            <div  class="display-block" >
                                <div id="categorySelectionCheck"class="new-btn profilenew-input-halfarea display-inline" {{action dropdown "category"}}>
                                     <span id="categorySelection">{{controller.categorySelection}}</span>
                                    <span class="caret"></span>
                                    {{#if controller.categoryDropdown}}
                                    {{view HubStar.DropdownListView}}
                                    {{/if}}
                                </div>
                                <div id="subcategorySelectionCheck" class="new-btn profilenew-input-halfarea2 display-inline" {{action dropdown "subcategory"}}>
                                     <span id="subcategorySelection">{{controller.subcategorySelection}}</span>
                                    <span class="caret"></span>
                                    {{#if controller.subcategoryDropdown}}
                                    {{view HubStar.DropdownListView}}
                                    {{/if}}
                                </div>                              
                            </div>
                        </div>
                    </div>
                <div class="table-row">
                    <div class="table-cell align-right" >Physical Address*:</div>                      
                    <div class="table-cell">
                        <div id="countrySelectionCheck" class="new-btn profilenew-inputarea" {{action dropdown "country"}}>
                             <span id="countrySelection">{{controller.countrySelection}}</span>
                            <span class="caret"></span>
                            {{#if controller.countryDropdown}}
                            {{view HubStar.DropdownListView}}
                            {{/if}}
                        </div>
                    </div>
                </div>
                    <div class="table-row">
                        <div class="table-cell align-right" ></div>
                        <div class="table-cell">
                            <div id="regionSelectionCheck" class="new-btn profilenew-inputarea" {{action dropdown "region"}}>
                                 <span id="regionSelection">{{controller.regionSelection}}</span>
                                <span class="caret"></span>
                                {{#if controller.regionDropdown}}
                                {{view HubStar.DropdownListView}}
                                {{/if}}
                            </div>                  
                        </div>
                    </div>
                    <div class="table-row">
                        <div class="table-cell align-right" ></div>
                        <div class="table-cell" > 
                            <div class="display-block">
                                <div class="profilenew-inputarea" >
                                    {{view Ember.TextField valueBinding="suburb"  placeholder="Suburb"}}
                                </div>
                            </div>
                       </div>
                    </div>
                    <div class="table-row">
                        <div class="table-cell align-right" ></div>
                        <div class="table-cell"> 
                            <div class="profilenew-inputarea">
                                {{view Ember.TextField valueBinding="address"  placeholder="Street Address"}}
                            </div>
                        </div>
                    </div>
              

                <div class="table-row">
                    <div class="table-cell align-right" >Contact Name:</div>
                    <div class="table-cell"> 
                        <div  class="display-block" >
                            <div class="profilenew-input-halfarea display-inline" >
                                {{view Ember.TextField valueBinding="first_name"  placeholder="First Name"}}
                            </div>
                            <div class="profilenew-input-halfarea2 display-inline">
                                {{view Ember.TextField valueBinding="last_name"  placeholder="Last Name"}}
                            </div>

                        </div>
                    </div>
                </div>
                <div class="table-row">
                    <div class="table-cell align-right" >Contact Number:</div>
                    <div class="table-cell"> 
                        <div class="display-block" >
                            <div id="numberField" class="profilenew-inputarea2 display-inline" >
                                {{view Ember.TextField valueBinding="profile_contact_number" class="contactNumber" placeholder="Your Business Contact Number"}}
                            </div>                          
                        </div>
                    </div>
                </div>
                <div class="table-row">
                    <div class="table-cell align-right" >Contact Email*:</div>
                    <div class="table-cell"> 
                        <div class="display-block" >
                            <div   id="contactEmailField" class="profilenew-inputarea2 display-inline">
                                {{view Ember.TextField   id="contactEmail" valueBinding="direct_enquiry_emails"  class="contactEmail"  placeholder="emailaddress@yourdomain.com"}}
                            </div>
                            <div class="display-inline" id="firstAdd" {{action addSecond target="view" on="click"}}><i class="fa fa-plus" ></i></div> 
                        </div>     
                        <div class="mustfull display-none" id="contactEmailFormat" >not correct email format.....</div>
                    </div>

                </div>

                <div  id="secondEmail" class="display-none" >
                    <div class="table-cell align-right" >Contact Second Email:</div>
                    <div class="table-cell"> 
                        <div class="display-block" >
                            <div   id="contactEmailField2" class="profilenew-inputarea2 display-inline">
                                {{view Ember.TextField valueBinding="direct_enquiry_emails_2"  id="contactEmail2"  class="contactEmail" placeholder="emailaddress@yourdomain.com"}}
                            </div>
                            {{#if view.added}}
                            <div class="display-inline" id="secondAdd" {{action addThird target="view" on="click"}}><i class="fa fa-plus" ></i></div>
                            <div class="display-inline" id="secondDelete" {{action deleteSecond target="view" on="click"}}><i class="fa fa-minus" ></i></div>
                            {{else}}
                            <div class="display-inline" id="secondDelete" {{action deleteSecond target="view" on="click"}}><i class="fa fa-minus" ></i></div>
                            {{/if}}
                        </div>     
                        <div class="mustfull display-none" id="contactEmailFormat2" >not correct email format.....</div>
                    </div>

                </div>

                <div id="thirdEmail" class="display-none">
                    <div class="table-cell align-right" >Contact Third Email:</div>
                    <div class="table-cell" > 
                        <div class="display-block" >
                            <div   id="contactEmailField3" class="profilenew-inputarea2 display-inline">
                                {{view Ember.TextField id="contactEmail3" valueBinding="direct_enquiry_emails_3"  class="contactEmail"  placeholder="emailaddress@yourdomain.com"}}
                            </div>
                            <div class="display-inline" id="thirdDelete" {{action deleteThird target="view" on="click"}}><i class="fa fa-minus" ></i></div>
                        </div>     
                        <div class="mustfull display-none" id="contactEmailFormat3" >not correct email format.....</div>
                    </div>


                </div>

                <div class="table-row">
                    <div class="table-cell align-right" >Website Name:</div>
                    <div class="table-cell"> 
                        <div class="display-block" >
                            <div class="profilenew-inputarea" >
                                {{view Ember.TextField valueBinding="website" placeholder="yourdomain.com"}}
                            </div>
                        </div>
                    </div>
                </div>

                <div class="table-row">
                    <div class="table-cell align-right" >Website URL:</div>
                    <div class="table-cell" > 
                        <div class="display-block">
                            <div class="profilenew-inputarea">
                                {{view Ember.TextField valueBinding="website_url" placeholder="http://www.yourdomain.com"}}
                            </div>
                        </div>
                    </div>
                </div>

                 <div class="table-row" id="addTR">
                    <div class="table-cell align-right" >Admins*:</div>
                    <div class="table-cell" > 
                        <div class="display-block">
                            <div   id="adminsField_1" class="profilenew-inputarea2 display-inline">
                                <input id="admins_1"  type="text" class="admins" placeholder="emailaddress@yourdomain.com"/>                           
                            </div>
                            <div class="display-inline" {{action addTRmore}}><i class="fa fa-plus" ></i></div>
                        </div>                   
                        <div class="mustfill display-none" id="adminsEmailFormat_1" >not correct email format.....</div>
                    </div>                 
                </div>
            </div>
         </div>

            <div class="display-block align-center profileNew-create" >
                <a class="new-btn green-btn" href="#" {{action save }} style="width: 100px;">Create Profile</a>
            </div>

        </div>
    </div>