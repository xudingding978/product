

<div class="review-list-container">
    {{#each reviews}} 



    <div id="reviewList"  class="review-list box" style="">


        {{#if isValid}}
        <div class="review-list-profile-pic" style="float:left;">
            <img {{bindAttr src="this.review_user_photo_url"}} id="leftOrRight" style=""/>
        </div>

        <div class="review-list-content" style="">

            <div class="comment-namentime">
                <div class="namentime_position">
                    <a class="comment-username" href="#/users/{{unbound commenter_id}}" {{action linkingUser commenter_id}} >
                      {{this.review_user_name}} 
                </a>
                <div class="posttime-container" style="display: inline-block;">
                    <span class="posttime">{{date this.review_time_stamp}} </span>
                </div>
            </div>
        </div>

  <span class="star-rating-control">
        <div id ="1" role="text" aria-label="" class="star-rating star-rating-live " >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >

           
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live">
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            

        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live"">
           
        </div>
  </span>


        <div>{{this.review_content}}   {{this.review_count}}</div>


    </div>

    {{else}}
    <div class="review-list-profile-pic" style="float:right;">
        <img {{bindAttr src="this.review_user_photo_url"}} id="leftOrRight" style=""/>
    </div>

    <div class="review-list-content" style="text-align: right;">
        <div class="comment-namentime">
            <div class="namentime_position" style="margin: 0;">
                <div class="posttime-container" style="display: inline-block;">
                    <span class="posttime">{{date this.review_time_stamp}} </span>
                </div>
                <a class="comment-username" href="#/users/{{unbound commenter_id}}" {{action linkingUser commenter_id}} >
                  {{this.review_user_name}}   
            </a>
        </div>
    </div>


      <div id ="1" role="text" aria-label="" class="star-rating star-rating-live " >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >

           
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live">
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            

        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live" >
            
        </div>
        <div role="text" aria-label="" class="star-rating  star-rating-live"">
           
        </div>

    <div>{{this.review_content}}   {{this.review_count}}</div>

</div>




{{/if}}

</div>
<script>
    $(function() {
        var $container = $('#masonry_user_container');
        window.setTimeout(function() {
            $container.masonry();
            $container.masonry('reload');
        }, 10);
    });
</script>


{{/each}}

</div>
